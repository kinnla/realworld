		ROUTE Sensor.exitTime TO FadeTimer.stopTime
		ROUTE Sensor.isActive TO FadeTimer.enabled
		ROUTE FadeTimer.fraction_changed TO Fader.set_fraction
		ROUTE Fader.value_changed TO Font.transparency

	}
  




